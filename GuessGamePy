import random

number = random.randint(1, 20)


sum = 0
while sum < 5:
    guess = int(input("Take a guess= " ))
    sum = sum + 1

    if guess > number:
    
        print("Guess high")

    if guess < number:
        print("guess low")

    if guess == number:
        print("Well Done")
        break

    

